label live-@FLAVOUR@
	menu label ^Dutch
	kernel @KERNEL@
	append initrd=@INITRD@ boot=live config @LB_BOOTAPPEND_LIVE@ locales=nl keyboard-layouts=nl

label live-@FLAVOUR@
